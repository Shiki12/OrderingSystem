<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shiki.dao.AdministratorDao">
    
    <select id="selectById" resultType="com.shiki.entity.Administrator" parameterType="int">
        select *
        from user where id=#{id};
    </select>
</mapper>